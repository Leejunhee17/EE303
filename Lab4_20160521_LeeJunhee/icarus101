    .scope S_00FD2B38;
T_6 ;
    %vpi_call 2 166 "$dumpfile", "myfile.dmp";
    %vpi_call 2 167 "$dumpvars";
    %set/v v00FDDBE8_0, 0, 4;
    %set/v v00FDDDF8_0, 0, 4;
    %set/v v00FDDDA0_0, 0, 4;
    %set/v v00FDDC40_0, 0, 1;
    %set/v v00FDDCF0_0, 1, 1;
    %movi 8, 100, 8;
T_6.0 %cmp/s 0, 8, 8;
    %jmp/0xz T_6.1, 5;
    %add 8, 1, 8;
    %wait E_00FD59C0;
