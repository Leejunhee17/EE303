t_12 ;
    %vpi_call 2 79 "$display", "Transmit Data";
    %set/v v00FDDBE8_0, 0, 4;
    %set/v v00FDDDF8_0, 0, 4;
    %set/v v00FDDDA0_0, 0, 4;
    %set/v v00FDDC40_0, 0, 1;
    %movi 8, 10, 5;
T_3.12 %cmp/s 0, 8, 5;
    %jmp/0xz T_3.13, 5;
    %add 8, 1, 5;
    %wait E_00FD59C0;
    %jmp T_3.12;
T_3.13 ;
    %delay 1000, 0;
    %set/v v01143948_0, 0, 32;
T_3.14 ;
    %load/v 8, v01143948_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_3.15, 5;
    %movi 8, 5, 32;
    %load/v 40, v01143948_0, 32;
    %movi 72, 10, 32;
    %mod/s 40, 72, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz  T_3.16, 5;
    %wait E_00FD59C0;
    %delay 1000, 0;
T_3.16 ;
    %ix/getv/s 3, v01143948_0;
    %load/av 8, v00FDDD48, 4;
    %set/v v00FDDBE8_0, 8, 4;
    %ix/getv/s 3, v01143948_0;
    %load/av 8, v00FDDF58, 4;
    %set/v v00FDDDF8_0, 8, 4;
    %ix/getv/s 3, v01143948_0;
    %load/av 8, v00FDE110, 4;
    %set/v v00FDDDA0_0, 8, 4;
    %set/v v00FDDC40_0, 1, 1;
    %wait E_00FD59C0;
    %delay 1000, 0;
    %set/v v00FDDC40_0, 0, 1;
