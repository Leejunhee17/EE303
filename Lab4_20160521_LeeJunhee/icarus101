T_5 ;
    %set/v v00FDDB68_0, 0, 1;
    %delay 1000, 0;
T_5.0 ;
    %load/v 8, v00FDDB68_0, 1;
    %inv 8, 1;
    %set/v v00FDDB10_0, 8, 1;
    %movi 8, 5000, 14;
    %ix/get 0, 8, 14;
    %delayx 0;
    %load/v 8, v00FDDB10_0, 1;
    %set/v v00FDDB68_0, 8, 1;
    %jmp T_5.0;
