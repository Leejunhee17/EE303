S_00FD29A0 .scope task, "err_check" "err_check" 2 133, 2 133, S_00FD2B38;
 .timescale -9 -12;
TD_TB.err_check ;
    %fork t_1, S_00FD34C8;
    %jmp t_0;
    .scope S_00FD34C8;
t_1 ;
    %vpi_call 2 137 "$display", "Error Check";
    %set/v v00FDDC98_0, 0, 32;
    %set/v v01143A50_0, 0, 32;
T_0.0 ;
    %load/v 8, v01143A50_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_0.1, 5;
    %ix/getv/s 3, v01143A50_0;
    %load/av 8, v00FDE168, 9;
    %ix/getv/s 3, v01143A50_0;
    %load/av 17, v00FDE1C0, 9;
    %cmp/u 8, 17, 9;
    %jmp/0xz  T_0.2, 4;
    %load/v 8, v00FDDC98_0, 32;
    %set/v v00FDDC98_0, 8, 32;
    %jmp T_0.3;
T_0.2 ;
    %load/v 8, v00FDDC98_0, 32;
    %mov 40, 39, 1;
    %addi 8, 1, 33;
    %set/v v00FDDC98_0, 8, 32;
T_0.3 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v01143A50_0, 32;
    %set/v v01143A50_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 150 "$display", "Error Count: %d", v00FDDC98_0;
    %load/v 8, v00FDDC98_0, 32;
    %cmpi/u 8, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %vpi_call 2 153 "$display", "Successfuly completed";
    %jmp T_0.5;
T_0.4 ;
    %vpi_call 2 155 "$display", "Failed";
T_0.5 ;
    %end;
    .scope S_00FD29A0;
t_0 %join;
    %end;
S_00FD34C8 .scope begin, "task_3" "task_3" 2 134, 2 134, S_00FD29A0;
 .timescale -9 -12;
v01143A50_0 .var/i "i", 31 0;
S_00FD3440 .scope task, "generate_data" "generate_data" 2 57, 2 57, S_00FD2B38;
 .timescale -9 -12;
TD_TB.generate_data ;
    %fork t_3, S_00FD2BC0;
    %jmp t_2;
    .scope S_00FD2BC0;
t_3 ;
    %vpi_call 2 61 "$display", "Generate Data";
    %set/v v011439F8_0, 0, 32;
T_1.6 ;
    %load/v 8, v011439F8_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_1.7, 5;
    %vpi_func 2 65 "$random", 8, 32;
    %ix/getv/s 3, v011439F8_0;
   %jmp/1 t_4, 4;
   %ix/load 1, 0, 0;
   %set/av v00FDDD48, 8, 4;
t_4 ;
    %vpi_func 2 66 "$random", 8, 32;
    %ix/getv/s 3, v011439F8_0;
   %jmp/1 t_5, 4;
   %ix/load 1, 0, 0;
   %set/av v00FDDF58, 8, 4;
t_5 ;
    %vpi_func 2 67 "$random", 8, 32;
    %ix/getv/s 3, v011439F8_0;
   %jmp/1 t_6, 4;
   %ix/load 1, 0, 0;
   %set/av v00FDE110, 8, 4;
t_6 ;
    %ix/getv/s 3, v011439F8_0;
    %load/av 8, v00FDDD48, 4;
    %mov 12, 0, 5;
    %ix/getv/s 3, v011439F8_0;
    %load/av 17, v00FDDF58, 4;
    %movi 21, 0, 5;
    %mul 8, 17, 9;
    %ix/getv/s 3, v011439F8_0;
    %load/av 17, v00FDE110, 4;
    %mov 21, 0, 5;
    %add 8, 17, 9;
    %ix/getv/s 3, v011439F8_0;
   %jmp/1 t_7, 4;
   %ix/load 1, 0, 0;
   %set/av v00FDE168, 8, 9;
t_7 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v011439F8_0, 32;
    %set/v v011439F8_0, 8, 32;
