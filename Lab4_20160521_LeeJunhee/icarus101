    %wait E_00FD59C0;
    %delay 1000, 0;
    %set/v v011439A0_0, 0, 32;
T_2.8 ;
    %load/v 8, v011439A0_0, 32;
   %cmpi/s 8, 1000, 32;
    %jmp/0xz T_2.9, 5;
T_2.10 ;
    %load/v 8, v00FDDE50_0, 1;
    %cmpi/u 8, 1, 1;
    %inv 6, 1;
    %jmp/0xz T_2.11, 6;
    %wait E_00FD5DA0;
    %jmp T_2.10;
T_2.11 ;
    %delay 1000, 0;
    %load/v 8, v00FDDEA8_0, 9;
    %ix/getv/s 3, v011439A0_0;
   %jmp/1 t_10, 4;
   %ix/load 1, 0, 0;
   %set/av v00FDE1C0, 8, 9;
t_10 ;
    %wait E_00FD59C0;
